{
  "name": "make-js-great-again",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Victor H",
  "license": "MIT",
  "engines": {
    "node": "8.x || 9.x"
  },
  "scripts": {
    "start:dev": "concurrently --kill-others \"yarn run start:server:dev\" \"yarn run start:client:dev\"",
    "start:server:dev": "nodemon ./app/server/src/index.js",
    "start:client:dev": "webpack-dev-server",
    "build": "yarn run lint && yarn run clean && cross-env NODE_ENV=production webpack --progress",
    "lint": "eslint app --color",
    "lint:fix": "yarn run lint --fix",
    "clean": "rimraf dist",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.2.8",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.3",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "webpack": "^3.9.1",
    "webpack-compile-time-plugin": "^1.0.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
